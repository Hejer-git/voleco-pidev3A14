<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="69.0" prefWidth="395.0" stylesheets="@ajouterpromo.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.offre.gui.PromoItemController">
   <children>
      <HBox layoutX="35.0" layoutY="14.0" prefHeight="45.0" prefWidth="347.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 10px;" styleClass="card-user" stylesheets="@promoitem.css">
         <children>
            <Label fx:id="idlabel" alignment="CENTER" prefHeight="45.0" prefWidth="0.0" text="Id" textAlignment="CENTER">
               <font>
                  <Font size="1.0" />
               </font>
            </Label>
            <Label fx:id="Codelabel" alignment="CENTER" prefHeight="45.0" prefWidth="97.0" text="Code Promo" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="Reductionlabel" alignment="CENTER" prefHeight="45.0" prefWidth="95.0" text="Reduction" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="bouttonModif" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#btnclicked" prefHeight="5.0" prefWidth="65.0" text="Modifier">
               <HBox.margin>
                  <Insets right="5.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <Button fx:id="bouttonsup" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#delbtn" prefHeight="25.0" prefWidth="76.0" text="Supprimer">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
         </children></HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
